{
  "name": "bookly-api",
  "version": "1.0.0",
  "description": "api for bookly",
  "main": "index.js",
  "scripts": {
    "build": "babel ./ -d build --ignore node_modules/ --presets=env --source-maps",
    "prebuild": "rm -rf build/*",
    "start": "NODE_ENV=production node ./build/index.js",
    "prestart": "npm run build",
    "dev": "NODE_ENV=development nodemon --exec babel-node index.js",
    "new": "nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BooklyTech/bookly-api.git"
  },
  "keywords": [
    "edtech",
    "book",
    "education",
    "learning"
  ],
  "author": "Bookly",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BooklyTech/bookly-api/issues"
  },
  "homepage": "https://github.com/BooklyTech/bookly-api#readme",
  "dependencies": {
    "aws-sdk": "^2.323.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.13",
    "multer": "^1.4.0",
    "multer-s3": "^2.7.0",
    "validator": "^10.7.1"
  },
  "devDependencies": {
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "nodemon": "^1.18.4"
  }
}
